    "reveal:css": "cp -R ./node_modules/reveal.js/css ./web/css",
    "reveal:lib": "cp -R ./node_modules/reveal.js/lib ./web/lib",
    "reveal:plugin": "cp -R ./node_modules/reveal.js/plugin ./web/plugin",
